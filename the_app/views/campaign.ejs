<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Magnus</title>
  <link rel="stylesheet" href="styles.css">
  <script src="http://code.jquery.com/jquery-latest.js"></script>
<script src="jquery.easyPaginate.js"></script>

<pre class="javascript">
  $('#easyPaginate').easyPaginate({
    paginateElement: 'div',
    elementsPerPage: 3,
    effect: 'climb'
  });
</pre>
</head>


<body>
<h1>Magnus Saved Images</h1>



    
<div class="line"></div>




  <% if (result.length == 0){ %>
    <P> Please add a star</P>
  <% }else{ %>
      <div id="easyPaginate">
        <% for(var i=0; i<result.length; i++) {%>
          <div class="img_container">
            <a href="<%= result[i].link %>">
            <img src="<%= result[i].link %>">
            </a>
          </div> 
        <% } %>
     </div>
  <% } %>








</body>
</html>
